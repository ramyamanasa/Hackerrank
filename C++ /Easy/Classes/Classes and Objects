
class Student{
    private:
        int grades[5];
    public:
        void input(){
            for (int i = 0; i < 5; i++){
                cin >> grades[i];
            }
        }
        int calculateTotalScore(){
            int sum = 0;
            for (int i = 0; i < 5; i++){
                sum += grades[i];
            }
            return sum;
        }
};
